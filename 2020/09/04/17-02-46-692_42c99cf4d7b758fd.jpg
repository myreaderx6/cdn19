  <script>

    function canvasReady() {
      $('.canvas').addClass('ready')
    }

    function controlImage($, previewLink, name) {
      var img = new Image()
      img.src = previewLink
      img.onload = function () {
        $('.canvas').html("<img src=" + img.src + " alt='" + name + "' />")
        canvasReady()
      }
      img.onerror = function() {
        $('.canvas').html('<article class="text-danger">پیش نمایش با مشکل مواجه شد</article>')
        canvasReady()
      }
    }

    function controlPlayer($, previewLink) {
      jwplayer('player').setup({
        file: previewLink,
        logo: {
          file: '/images/pg.png',
          link: 'http:/cdn.persiangig.com:3000',
          margin: 8,
          position: 'top-left'
        },
        abouttext: 'Persiangig.com',
        aboutlink: 'http://persiangig.com'
      })

      jwplayer().onReady(function() {
        canvasReady()
      })

      jwplayer().onPlay(function() {
        sendReport('DownloadPage-Play', 'Play Button Clicked', 'Play')
      })
    }

    function toggleDownloadButton(disableStatus) {
      const activeText = 'دانلود'
      const disabledText = 'در حال دانلود'
      jQuery('#js-download').attr('disabled', disableStatus).html(disableStatus ? disabledText : activeText)
    }

    function showMessage(message) {
      jQuery('#js-message').html('').removeClass('hidden').html(message)
    }

    function hideMessage() {
      jQuery('#js-message').addClass('hidden').html('')
    }

    function showModalMessage(message) {
      jQuery('#js-modal-message').html('').removeClass('hidden').html(message)
    }

    function hideModalMessage() {
      jQuery('#js-modal-message').html('').addClass('hidden')
    }

    function getDownloadToken(uuid, name) {
      toggleDownloadButton(true)

      jQuery.getJSON('http://cdn.persiangig.com/cfs/rest/publicAccess/' + uuid + '/generateDownloadLink', function (res) {
        window.location.href = 'http://cdn.persiangig.com/dl/' + res.token + '/' + uuid + '/' + name
        toggleDownloadButton(false)
      }, function (err) {
        toggleDownloadButton(false)
        showMessage(err)
        setTimeout(function () {
          hideMessage()
        }, 2500)
      })
    }

    function showModal(uuid) {
      jQuery('#js-modal-broken-link').modal()
    }

    function autoHeightCanvas() {
      jQuery('.canvas').css('height', 'auto')
    }

    function requestSpamLink(form) {
      var txt = jQuery('#broken-link').val()
      showModalMessage(' متن شما ' + txt)
      return false
    }

    function sendReport(category, action, label) {
      ga('send', 'event', category, action, label)
    }

    function copyToClipboard() {
      var copyTextareaBtn = document.querySelector('.embed-code-container');

      copyTextareaBtn.addEventListener('click', function(event) {
        var copyTextarea = document.querySelector('.embed-code');
        copyTextarea.select();

        try {
          var successful = document.execCommand('copy');
          var msg = successful ? 'successful' : 'unsuccessful';
        } catch (err) {
          console.log('Oops, unable to copy');
        }
      });
    }

    jQuery(document).ready(function() {
      jQuery('#js-top-ad').on('click', function() {
        sendReport('DownloadPage-Ad', 'Top Ad Click', 'Click Ad')
      })
      jQuery('#js-left-ad').on('click', function() {
        sendReport('DownloadPage-Ad', 'Left Ad Click', 'Click Ad')
      })
      jQuery('#js-right-ad').on('click', function() {
        sendReport('DownloadPage-Ad', 'Right Ad Click', 'Click Ad')
      })
      jQuery('#js-bottom-ad').on('click', function() {
        sendReport('DownloadPage-Ad', 'Bottom Ad Click', 'Click Ad')
      })
    })

    jQuery(document).on('hidden.bs.modal', function () {
      jQuery('#broken-link').val('')
    })

  </script>
